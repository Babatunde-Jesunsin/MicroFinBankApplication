@model IEnumerable<BankData.Entities.AccountStatement>

@{
    ViewBag.Title = "ViewStatement";
}

<h2>ViewStatement</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AccountNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StatementOfAccount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModifyAt)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AccountNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StatementOfAccount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModifyAt)
        </td>
       
    </tr>
}

</table>
